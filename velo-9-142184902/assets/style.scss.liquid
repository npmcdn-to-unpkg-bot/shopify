	padding:1em;
	border-radius:5px
}

.swatch.error p{
	margin:0.7em 0
}

.swatch.error p:first-child{
	margin-top:0
}

.swatch.error p:last-child{
	margin-bottom:0
}

.swatch.error code{
	font-family:monospace
}


