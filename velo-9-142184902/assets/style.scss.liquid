	content: '/';
	right: 0;
	position: absolute;
	color: #e2e2e2;
}
.sub__menu {
	position: absolute;
	width: 200px;
	text-align: left;
	background: #fff;
	left: -10px;
	top: 120%;
	opacity: 0;
	visibility: hidden;
	transition: all .3s;
}
.has__sub > a:before {
	position: absolute;
	content: '\e80f';
	font-family: 'fontello';
	right: -16px;
    top: 0;
	font-size: 13px;
	color: #a2a2a2;
    line-height: 100%;
}
.has__sub.sub__mega > a {
	position: relative;
}
.sub__menu .has__sub > a:before {
	content: '\e80e';
	right: 10px;
	top: 8px;
}
.has__sub:hover > .sub__menu {
	opacity: 1;
	visibility: visible;
	top: 100%;
}
.sub__menu .menu__item {
	width: 100%;
}
.sub__menu .menu__item a {
	display: block;
	padding: 10px;
}
.sub__menu ul {
	left: 220px;
	top: 0;
}
.sub__menu .has__sub:hover > .sub__menu {
	left: 200px;
	top: 0;
}
.sub__mega {
	position: static;
}
.sub__mega .sub__menu {
	max-width: 1200px;
	width: 100%;
	left: calc(50% - 600px);
	padding: 20px 0;
}
.sub__mega .mega--title {
	margin-bottom: 15px;
	font-size: 25px;
}
.sub__mega .sub__menu .menu__item a {
	padding: 10px 0;
	text-transform: none;
}
.menu--rotate {
	width: 17px;
	height: 12px;
	position: relative;
	margin-left: 32px;
}
.menu--rotate:hover {
	-webkit-transform: rotate(180deg);
	transform: rotate(180deg);
}
.menu--rotate:hover .menu__bar:before {
	width: 10px;
	-webkit-transform: translateX(1px) translateY(-1px) rotate(-45deg);
	transform: translateX(1px) translateY(-1px) rotate(-45deg);
}
.menu--rotate:hover .menu__bar:after {
	-webkit-transform: translateX(1px) translateY(1px) rotate(45deg);
	transform: translateX(1px) translateY(1px) rotate(45deg);
}
.menu__bar,
.menu__bar:before,
.menu__bar:after {
	background: #737373;
	position: absolute;
	width: 100%;
	height: 2px;
	transition: all .3s;
}
.menu__bar {
	left: calc(50% - 10px);
	top: calc(50% - 1px);
}
.menu__bar:before,
.menu__bar:after {
	content: '';
	right: 0;
	top: 0;
}
.menu__bar:before {
	-webkit-transform-origin: right top;
	transform-origin: right top;
	-webkit-transform: translateY(-5px);
	transform: translateY(-5px);
}
.menu__bar:after {
	width: 10px;
	-webkit-transform-origin: right bottom;
	transform-origin: right bottom;
	-webkit-transform: translateY(5px);
	transform: translateY(5px);
}

/* ==========================================================================
   MENU 3D
   ========================================================================== */
.menu-opened {
	overflow: hidden;
}
.animated {
	-webkit-perspective: 1500px;
	perspective: 1500px;
	transition: background 0.6s;
}
.animated .wrapper {
	transition: transform .5s;
	-webkit-transform-origin: 0 100%;
	transform-origin: 0 100%;
	-webkit-transform: translateZ(0) translateX(0) rotateX(0);
	transform: translateZ(0) translateX(0) rotateX(0);
}
.effect-laydown {
	background: #f0f0f0;
}
.effect-laydown .mask-overlay {
	display: none
}
.mask-overlay {
	content: '';
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	background: rgba(0, 0, 0, .95);
	z-index: 100;
}
.effect-moveleft:before {
	opacity: 1;
	visibility: visible;
}
.effect-laydown .wrapper,
.effect-moveleft .wrapper {
	cursor: pointer;
	-webkit-backface-visibility: hidden;
	backface-visibility: hidden;
}
.effect-laydown .wrapper {
	opacity: .4;
	padding: 0;
	overflow: hidden;
	-webkit-transform: translateZ(-600px) translateX(0) rotateX(90deg);
    transform: translateZ(-600px) translateX(0) rotateX(90deg);
}
.effect-laydown .site-content {
	padding: 0;
}
.effect-moveleft .wrapper {
	overflow: hidden;
	webkit-transform: translateX(-20%) rotateY(20deg) translateZ(-50px);
	transform: translateX(-20%) rotateY(20deg) translateZ(-50px);
	-webkit-transform-origin: 50% 50%;
	transform-origin: 50% 50%;
}
.menu-close,
.search-close {
	position: absolute;
	right: 20px;
	top: 10px;
	color: #717171;
	font-size: 32px;
	z-index: 101;
}
.effect-moveleft .menu-close {
	color: #fff;
}
.effect-moveleft .menu-close:hover {
	color: #2380ff;
}
.outer-nav {
	top: 50%;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
	position: absolute;
	right: 20%;
	z-index: 101;
	pointer-events: none;
}
.menu-opened .outer-nav {
	pointer-events: initial;
}
.menu__right--item {
	position: relative;
	-webkit-transform: translateX(100px);
	transform: translateX(100px);
	opacity: 0;
	visibility: hidden;
	transition: all .4s;
}
.menu-right .menu__right--item {
	-webkit-transform: translateX(0);
	transform: translateX(0);
	opacity: 1;
	visibility: visible;
}
.menu__right--item:not(:last-child) {
	margin-bottom: 5px;
}
.menu__right--item:nth-child(2) {
	transition-delay: .04s;
}
.menu__right--item:nth-child(3) {
	transition-delay: .08s;
}
.menu__right--item:nth-child(4) {
	transition-delay: .12s;
}
.menu__right--item:nth-child(5) {
	transition-delay: .16s;
}
.menu__right--item:nth-child(6) {
	transition-delay: .20s;
}
.menu__right--item:nth-child(7) {
	transition-delay: .24s;
}
.menu__right--item:nth-child(8) {
	transition-delay: .28s;
}
.menu__right--item:nth-child(9) {
	transition-delay: .32s;
}
.menu__right--item:nth-child(10) {
	transition-delay: .36s;
}
.menu__right--item a {
	display: block;
	color: #fff;
	font-size: 35px;
	text-transform: uppercase;
}
.menu__right--item a:before {
	content: '';
	position: absolute;
	bottom: 0;
	left: 0;
	width: 0;
	height: 1px;
	background: #2380ff;
	transition: all .3s;
}
.menu__right--item a:hover:before {
	width: 100%;
}
.menu__right--item a:hover {
	color: #2380ff;
}

