	background: #f5f5f5;
}
.wrapper:before {
	content: '';
	border-left: 30px solid #fff;
	border-right: 30px solid #fff;
	position: fixed;
	width: calc(100% - 60px);
	left: 0;
	top: 0;
	bottom: 0;
	z-index: 100;
	pointer-events: none;
}
.fixed {
	width: calc(100% - 60px);
	position: fixed;
	background: #fff;
	z-index: 101;
}
.btn {
	border: 0;
	font-family: 'Akzidenz Grotesk CDS';
	font-weight: normal;
	text-transform: uppercase;
	font-size: 16px;
	border-radius: 3px;
	cursor: pointer;
	letter-spacing: .5px;
	transition: all .3s;
}
.btn--md {
	height: 40px;
	line-height: 40px;
	padding: 0 30px;
}
.btn--lg {
	height: 50px;
	line-height: 50px;
	padding: 0 40px;
}
.btn--blue {
	background: #2380ff;
	color: #fff;
}
.btn--blue:hover {
	background: #0d6ef2;
	box-shadow: 0px 4px 15px 0px rgba(35, 128, 255, 0.3),0px 4px 22px 0px rgba(201, 201, 201, 0.2);
}
.btn--line {
	background: none;
	border: 1px solid #d7dee4;
	color: #575757;
}
.btn--line:hover {
	color: #2380ff;
	border-color: #2380ff;
}
.btn--black {
	background: #444;
	color: #fff;
}
.btn--black:hover {
	color: #fff;
	background: #2380ff;
}
.btn--white {
	box-shadow: 0 0 21px 0 rgba(169, 181, 200, .35)
}
.btn--radius {
	border-radius: 50px;
}
.select__list {
	position: absolute;
	display: none;
  	top: 100%;
  	left: 0;
  	opacity: 0;
    &.show {
      display: block;
      opacity: 1;
    }
}
.input__style {
	position: relative;
	padding-left: 25px;
	cursor: pointer;
}
.input__style input {
	display: none;
}
.input__style span:before,
.input__style span:after {
	content: '';
	position: absolute;
	left: 0;
	top: 2px;
}
.input__style span:before {
	width: 14px;
	height: 14px;
	border: 1px solid #ccc;
	border-radius: 1px;
}
.input__style span:after,
.input__style.selected span:after {
	width: 8px;
	height: 8px;
	background: #2380ff;
	transition: all .3s;
	top: 6px;
	left: 4px;
	-webkit-transform: scale(0);
	transform: scale(0);
}
.input__style input[type="checkbox"]:checked + span:after,
.input__style.selected input[type="checkbox"] + span:after {
	-webkit-transform: scale(1);
	transform: scale(1);
}

